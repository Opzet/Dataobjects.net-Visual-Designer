<?xml version="1.0"?>
<configuration>

    <configSections>
        <section name="Xtensive.Orm" type="Xtensive.Orm.Configuration.Elements.ConfigurationSection, Xtensive.Orm" />
    </configSections>

    <system.web>
        <compilation debug="true" targetFramework="4.0" />
    </system.web>

    <!--  DataObjects.Net configuration -->
    <Xtensive.Orm>
        <domains>
            <domain name="Default" upgradeMode="Recreate" connectionUrl="sqlserver://sa:halo@localhost/doemd">
                <types>
                    <add assembly="TXSoftware.DataObjectsNetEntityModel.Web.Model" />
                </types>
                <sessions>
                    <!--<session name="Default" options="LegacyProfile" />-->
                    <session name="Default" options="ServerProfile" />
                </sessions>
            </domain>
        </domains>
    </Xtensive.Orm>

    <system.webServer>
        <modules runAllManagedModulesForAllRequests="true">
            <add name="SessionManager" type="Xtensive.Practices.Web.SessionManager, Xtensive.Practices.Web" />
            <add name="UrlRoutingModule" type="System.Web.Routing.UrlRoutingModule, System.Web, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b03f5f7f11d50a3a" />
        </modules>
    </system.webServer>

    <system.serviceModel>
        <serviceHostingEnvironment aspNetCompatibilityEnabled="true"/>
        <standardEndpoints>
            <webHttpEndpoint>
                <!-- 
            Configure the WCF REST service base address via the global.asax.cs file and the default endpoint 
            via the attributes on the <standardEndpoint> element below
        -->
                <standardEndpoint name="" helpEnabled="true" automaticFormatSelectionEnabled="true"/>
            </webHttpEndpoint>
        </standardEndpoints>
    </system.serviceModel>

</configuration>
